{
  "name": "osspid-client",
  "version": "1.0.0",
  "description": "OSSPID demo client project which will interact with Keycloak and OSSPID adapter",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "dev": "nodemon index.js",
    "stop": "docker-compose down",
    "logs": "docker-compose logs -f",
    "restart": "docker-compose restart",
    "build": "docker-compose build",
    "rebuild": "docker-compose down && docker-compose up -d --build",
    "status": "docker ps --filter name=osspid-client",
    "shell": "docker exec -it osspid-client sh"
  },
  "author": "shamimshakir",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
